<?xml version="1.0"?>
<package format="2">
    <name>march_launch</name>
    <version>0.0.0</version>
    <description>Launches the related march packages</description>

    <!-- One maintainer tag required, multiple allowed, one person per tag -->
    <!-- Example:  -->
    <!-- <maintainer email="jane.doe@example.com">Jane Doe</maintainer> -->
    <maintainer email="software@projectmarch.nl">Project March</maintainer>

    <license>TODO</license>

    <buildtool_depend>catkin</buildtool_depend>
    <build_depend>march_control</build_depend>
    <build_depend>march_description</build_depend>
    <build_depend>march_gazebo</build_depend>
    <build_depend>march_state_machine</build_depend>
    <build_depend>march_state_machine_service</build_depend>
    <build_depend>rostest</build_depend>

    <build_export_depend>rviz</build_export_depend>
    <build_export_depend>rqt_gui</build_export_depend>
    <build_export_depend>march_gazebo</build_export_depend>
    <build_export_depend>march_control</build_export_depend>
    <build_export_depend>march_state_machine</build_export_depend>
    <build_export_depend>march_state_machine_service</build_export_depend>
    <build_export_depend>march_description</build_export_depend>

</package>
